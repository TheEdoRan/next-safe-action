{
	"$schema": "https://turborepo.dev/schema.json",
	"ui": "stream",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "!.next/cache/**", "dist/**", "build/**"]
		},
		"test": {},
		"lint": {},
		"deploy": {
			"cache": false,
			"dependsOn": ["build", "lint"],
			"passThroughEnv": ["GITHUB_TOKEN", "ACTIONS_ID_TOKEN_REQUEST_URL", "ACTIONS_ID_TOKEN_REQUEST_TOKEN"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
