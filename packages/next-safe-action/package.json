		"semantic-release": "^23.0.8",
		"tsup": "^8.0.2",
		"typescript": "^5.4.5",
		"typescript-eslint": "^7.7.0",
		"zod": "^3.23.4"
	},
	"peerDependencies": {
		"next": ">= 14.3.0-canary.36",
		"react": ">= 18.2.0"
